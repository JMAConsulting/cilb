<?xml version="1.0"?>
<extension key="uplang" type="module">
  <file>uplang</file>
  <name>Update Language Files</name>
  <description>Automated download of translations files for CiviCRM core and extensions.</description>
  <urls>
    <url desc="Main Extension Page">https://lab.civicrm.org/extensions/uplang</url>
    <url desc="Documentation">https://lab.civicrm.org/extensions/uplang/-/blob/master/README.md</url>
    <url desc="Support">https://lab.civicrm.org/extensions/uplang/-/issues</url>
  </urls>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Mathieu Lutfy</author>
    <email>mathieu@symbiotic.coop</email>
  </maintainer>
  <releaseDate>2024-01-10</releaseDate>
  <version>1.4.1</version>
  <compatibility>
    <ver>5.65</ver>
  </compatibility>
  <comments></comments>
  <civix>
    <namespace>CRM/Uplang</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
