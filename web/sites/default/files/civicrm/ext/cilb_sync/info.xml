<?xml version="1.0"?>
<extension key="cilb_sync" type="module">
  <file>cilb_sync</file>
  <name>CILB Exam Data Sync</name>
  <description>Sync exams data such as score and pass/fail result from different files.</description>
  <license>AGPL-3.0</license>
  <authors>
    <author>
      <name>JMA Consulting</name>
      <email>support@jmaconsulting.biz</email>
      <role>Maintainer</role>
    </author>
  </authors>
  <urls>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-09-15</releaseDate>
  <version>1.0.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>6.4</ver>
  </compatibility>
  <smarty_compatibility>
    <ver>5</ver>
  </smarty_compatibility>
  <php_compatibility>
    <ver>8.1</ver>
    <ver>8.2</ver>
    <ver>8.3</ver>
  </php_compatibility>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/CILB_Sync</namespace>
    <format>25.01.1</format>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
</extension>
