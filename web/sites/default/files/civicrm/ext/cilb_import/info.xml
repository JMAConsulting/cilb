<?xml version="1.0"?>
<extension key="cilb_import" type="module">
  <file>cilb_import</file>
  <name>cilb_import</name>
  <description>Import CILB data from legacy database into CiviCRM</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Ben</author>
    <email>ben@jmaconsulting.biz</email>
  </maintainer>
  <releaseDate>2024-09-16</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.75</ver>
  </compatibility>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Cilb_Import</namespace>
    <format>24.09.1</format>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>entity-types-php@2.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
  </mixins>
  <upgrader>CiviMix\Schema\CilbImport\AutomaticUpgrader</upgrader>
</extension>
