<?xml version="1.0"?>
<extension key="ses" type="module">
  <file>ses</file>
  <name>Amazon SES</name>
  <description>Amazon Simple Email Service (SES) integration</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Matthew Wire</author>
    <email>mjw@mjwconsult.co.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://lab.civicrm.org/extensions/ses</url>
    <url desc="Documentation">https://lab.civicrm.org/extensions/ses/-/blob/main/README.md</url>
    <url desc="Support">https://mjw.pt/support/ses</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-12-03</releaseDate>
  <version>1.3.14</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>6.4</ver>
  </compatibility>
  <civix>
    <namespace>CRM/Ses</namespace>
    <format>25.10.2</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@2.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>setting-admin@1.0.1</mixin>
    <mixin>entity-types-php@2.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
    <mixin>scan-classes@1.0.0</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
